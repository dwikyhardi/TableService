<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="root.example.com.tableservice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Waiter.WaiterDashboard"
            android:label="@string/title_activity_waiter_dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Owner.OwnerDashboard"
            android:label="@string/title_activity_owner_dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Customer.CustomerDashboard"
            android:label="@string/title_activity_customer_dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Cashier.CashierDashboard"
            android:label="@string/title_activity_cashier_dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Admin.AdminDashboard"
            android:label="@string/title_activity_admin_dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>